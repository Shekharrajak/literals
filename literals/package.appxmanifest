          <m2:Rotation Preference="landscapeFlipped" />
        </m2:InitialRotationPreference>
        <m2:ApplicationView MinWidth="default" />
      </m2:VisualElements>
      <Extensions>
        <Extension Category="windows.fileTypeAssociation">
          <FileTypeAssociation Name="literals">
            <DisplayName>Literals</DisplayName>
            <InfoTip>Edit with Literals</InfoTip>
            <SupportedFileTypes>
              <FileType>.txt</FileType>
              <FileType>.rst</FileType>
              <FileType>.md</FileType>
              <FileType>.rdoc</FileType>
              <FileType>.tex</FileType>
              <FileType>.diff</FileType>
              <FileType>.xml</FileType>
